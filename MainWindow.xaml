<Window x:Class="SwarmRoboticsGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SwarmRoboticsGUI"
        mc:Ignorable="d"
        Title="Swarm Robotics Graphics User Interface" Height="350" Width="525" Loaded="Window_Loaded">
    <Grid x:Name="grid1">
        <Grid.ColumnDefinitions>
            <ColumnDefinition x:Name="Column1" Width="*"/>
            <ColumnDefinition x:Name="Column2" Width="5"/>
            <ColumnDefinition x:Name="Column3" Width="*"/>
        </Grid.ColumnDefinitions>

        <GridSplitter Grid.Column="1" Width="5" HorizontalAlignment="Stretch"/>
    </Grid>
</Window>
