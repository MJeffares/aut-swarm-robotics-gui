<Window x:Class="SwarmRoboticsGUI.CameraPopOutWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SwarmRoboticsGUI"
        xmlns:em="clr-namespace:Emgu.CV.UI;assembly=Emgu.CV.UI"
        mc:Ignorable="d"
        
        Title="Swarm Robotics Camera Feed" Height="480" Width="640" Closed="CameraPopOutWindow_Closed">
    <Grid>
        <WindowsFormsHost Grid.Column="1" Name="host1" HorizontalAlignment="Stretch" Height="Auto" VerticalAlignment="Stretch" Width="Auto" Margin="0,0,0,0" Visibility="Visible">
            <em:ImageBox x:Name="captureImageBox" Height="640" Width="480" SizeMode="StretchImage" FunctionalMode="Minimum"/>
        </WindowsFormsHost>
    </Grid>
</Window>
